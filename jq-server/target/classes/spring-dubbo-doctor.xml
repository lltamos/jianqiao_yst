<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
	default-lazy-init="true">

	<!-- 声明需要暴露的服务接口 不需要webservice注解，只需要正常的服务类即可 -->
	<dubbo:service interface="com.alqsoft.rpc.RpcDoctorService"
		ref="rpcDoctorServiceImpl"  timeout="1200000" />
		<dubbo:service interface="com.alqsoft.rpc.RpcImDoctorServices"
		ref="rpcImDoctorServicesImpl"  timeout="1200000" />
</beans>